<div class="weatherWidget">
  {{#each partials.weatherData.locations}}
    <div class="location">
      <h3>{{name}}</h3>
      <a href="{{forecastUrl}}">
        <img src="{{iconUrl}}" alt="{{weather}}">
        {{weather}}, {{temp}}
      </a>
    </div>
  {{/each}}
  <small>
    <button class="threeDay" type="button">Three day</button>
    <button class="sevenDay" type="button">Seven day</button>
    Source: <a href="http://www.wunderground.com">Weather Underground</a>
  </small>
</div>